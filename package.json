{
  "name": "kazuvin-playground",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "generate:notes": "node scripts/generate-notes-data.mjs",
    "dev": "pnpm generate:notes && next dev --turbopack",
    "build": "pnpm generate:notes && next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.11.0",
    "next": "15.4.6",
    "next-mdx-remote": "^5.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "15.4.6",
    "@tailwindcss/postcss": "^4",
    "@types/mdx": "^2.0.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "eslint-plugin-react-hooks": "^7.0.1",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.11.1",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4.45.3"
  }
}
